{
  "name": "JSON Viewer Plus",
  "version": "1.0.2",
  "manifest_version": 2,
  "author": "Quentin Lienhardt",
  "description": "The most beautiful and customizable JSON/JSONP highlighter that your eyes have ever seen.",
  "homepage_url": "https://github.com/QuentiumYT/JSONViewerPlus",
  "minimum_chrome_version": "100",
  "icons": {
    "128": "icons/128.png",
    "32": "icons/32.png",
    "16": "icons/16.png"
  },
  "web_accessible_resources": [
    "assets/viewer.css",
    "assets/viewer-alert.css",
    "pages/options.html",
    "pages/omnibox.html"
  ],
  "offline_enabled": true,
  "omnibox": {
    "keyword": "json-viewer"
  },
  "options_page": "pages/options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/viewer.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "assets/backend.js",
      "assets/omnibox.js"
    ],
    "persistent": false
  },
  "permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}
